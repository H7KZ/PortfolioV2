{
	"name": "portfolio",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"update-all": "pnpm update && pnpm upgrade && pnpm install && pnpm update --force && pnpm upgrade --force && pnpm install --force",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"precommit": "prettier --plugin-search-dir . --write . && prettier --plugin-search-dir . --check . && eslint . && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "1.0.0-next.91",
		"@sveltejs/adapter-cloudflare": "^1.0.0",
		"@sveltejs/kit": "1.0.1",
		"@types/aos": "^3.0.4",
		"@types/eventsource": "^1.1.11",
		"@typescript-eslint/eslint-plugin": "^5.59.0",
		"@typescript-eslint/parser": "^5.59.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.38.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.22",
		"prettier": "^2.8.7",
		"prettier-plugin-svelte": "^2.10.0",
		"sass": "^1.62.0",
		"svelte": "^3.58.0",
		"svelte-check": "^2.10.3",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.3.1",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.2.2"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-cloudflare": "^1.1.0",
		"aos": "3.0.0-beta.6",
		"node-sass": "^8.0.0",
		"socket.io-client": "^4.6.1"
	}
}
